---
import Layout from '../layouts/base.astro';
import { ModelsPage } from '../components/ModelsPage';
---

<Layout
  title="Models - Free LLM Router"
  description="View model health and availability data for free LLM models from OpenRouter."
>
  <div class="container mx-auto max-w-5xl px-4 py-8">
    <ModelsPage client:load />

    <!-- Footer -->
    <footer class="mt-16 pt-8 text-center text-sm text-muted-foreground">
      <p>Data from OpenRouter sync and community reports</p>
    </footer>
  </div>
</Layout>
