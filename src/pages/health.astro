---
import Layout from '../layouts/base.astro';
import { IssuesPage } from '../components/IssuesPage';
---

<Layout
  title="Model Health - Free LLM Router"
  description="View model health data for free LLM models including reported issues with rate limits, availability, and errors."
>
  <div class="container mx-auto max-w-5xl px-4 py-8">
    <IssuesPage client:load />

    <!-- Footer -->
    <footer class="mt-16 pt-8 text-center text-sm text-muted-foreground">
      <p>Health data from issues reported by API users</p>
    </footer>
  </div>
</Layout>
